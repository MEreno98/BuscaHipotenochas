<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid
                android:color="#EEEEEE" />
            <stroke
                android:width="1px"
                android:color="#9E9E9E" />
            <corners
                android:radius="1px" />
            <padding
                android:left="0dp"
                android:top="0dp"
                android:right="0dp"
                android:bottom="0dp" />
        </shape>
    </item>
    <item>
        <shape>
            <solid
                android:color="#E0E0E0" />
            <stroke
                android:width="1px"
                android:color="#9E9E9E" />
            <corners
                android:radius="1px" />
            <padding
                android:left="0dp"
                android:top="0dp"
                android:right="0dp"
                android:bottom="0dp" />
        </shape>
    </item>
</selector>